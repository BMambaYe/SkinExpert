<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_start_test"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zhanghao.skinexpert.Activity.StartTestActivity">

    <RelativeLayout
        android:id="@+id/rl_startTest"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white"
        android:gravity="center_vertical">

        <Button
            android:id="@+id/btn_startTest"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:background="@mipmap/ic_return_back"
            android:onClick="onClick" />
    </RelativeLayout>

    <WebView
        android:id="@+id/wv_startTest"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/rl_startTest" />

    <Button
        android:id="@+id/btn_startTest_jump"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:alpha="0"
        android:onClick="onClick" />

</RelativeLayout>
